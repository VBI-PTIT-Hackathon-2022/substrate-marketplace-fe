{
  "name": "NFT.owl",
  "version": "0.1",
  "private": true,
  "license": "Unlicense",
  "keywords": [
    "substrate",
    "substrate-ui",
    "polkadot-js"
  ],
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "CI=true react-app-rewired test",
    "eject": "react-scripts eject",
    "eslint:check": "eslint --ext .jsx,.js src/",
    "eslint:write": "eslint --fix --ext .jsx,.js src/",
    "prettier:check": "prettier -c src/*",
    "prettier:write": "prettier -w src/*",
    "deploy": "gh-pages -d build -m '[ci skip] Updates'"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@pinata/sdk": "^2.1.0",
    "@polkadot/api": "^7.10.1",
    "@polkadot/extension-dapp": "^0.44.6",
    "@polkadot/keyring": "^8.4.1",
    "@polkadot/networks": "^8.4.1",
    "@polkadot/types": "^7.10.1",
    "@polkadot/ui-keyring": "^1.1.1",
    "@polkadot/ui-settings": "^1.1.1",
    "@polkadot/util": "^8.4.1",
    "@polkadot/util-crypto": "^8.4.1",
    "axios": "^1.1.3",
    "bootstrap": "^5.2.2",
    "dotenv": "^16.0.3",
    "elegant-icons": "^0.0.1",
    "et-line": "^1.0.1",
    "font-awesome": "^4.7.0",
    "moment": "^2.29.4",
    "node-polyfill-webpack-plugin": "^1.1.4",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-animated-slider": "^2.0.0",
    "react-awesome-reveal": "^4.1.0",
    "react-copy-to-clipboard": "^5.0.4",
    "react-dom": "^17.0.2",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.4.3",
    "react-scripts": "^5.0.0",
    "react-slick": "^0.29.0",
    "redux": "^4.2.0",
    "redux-thunk": "^2.4.2",
    "reselect": "^4.1.7",
    "sass": "^1.56.1",
    "semantic-ui-css": "Semantic-Org/Semantic-UI-CSS#master",
    "semantic-ui-react": "^2.1.2",
    "slick-carousel": "^1.8.1",
    "stream-browserify": "^3.0.0",
    "styled-components": "^5.3.6",
    "typesafe-actions": "^5.1.0"
  },
  "devDependencies": {
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-config-react-app": "^7.0.0",
    "gh-pages": "^3.2.3",
    "prettier": "2.5.1",
    "react-app-rewired": "^2.2.1",
    "semistandard": "^16.0.1"
  },
  "resolutions": {
    "eslint-plugin-jsx-a11y": "6.5.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "eslint:recommended",
      "prettier"
    ]
  },
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not ie <= 99",
      "not android <= 4.4.4",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "browser": {
    "crypto": false,
    "fs": false,
    "net": false,
    "os": false,
    "path": false,
    "stream": false,
    "tls": false
  },
  "packageManager": "yarn@3.1.1"
}
